# Copy to .env and fill values
GEMINI_API_KEY=your_gemini_key_here

# Either DATABASE_URL or the individual PG* vars
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/chatbot

# (Optional if you don't use DATABASE_URL)
PGHOST=localhost
PGPORT=5432
PGDATABASE=chatbot
PGUSER=postgres
PGPASSWORD=postgres
